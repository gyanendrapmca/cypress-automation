{
    "compilerOptions": {
        "target": "es2016",
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ],
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": [
        "**/*.ts"
    ]
}